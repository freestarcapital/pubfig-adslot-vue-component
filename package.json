{
  "name": "@freestar/pubfig-adslot-vue-component",
  "description": "Freestar Pubfig AdSlot Vue Component",
  "keywords": [
    "freestar",
    "pubfig",
    "adslot-vue-component"
  ],
  "version": "1.0.2",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/freestarcapital/pubfig-adslot-vue-component.git"
  },
  "bugs": {
    "url": "https://github.com/freestarcapital/pubfig-adslot-vue-component/issues"
  },
  "homepage": "https://github.com/freestarcapital/pubfig-adslot-vue-component#readme",
  "license": "ISC",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.min.js",
  "browser": {
    "./sfc": "src/components/freestarAdSlot.vue"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/index.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/index.esm.js",
    "build:unpkg": "rollup --config rollup.config.js --format iife --file dist/index.min.js",
    "publish": "npm publish --access public"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "@vue/compiler-sfc": "^3.0.5",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "rollup": "^2.38.4",
    "rollup-plugin-vue": "^6.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
